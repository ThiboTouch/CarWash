{
  "name": "carwash-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "mock:server": "json-server --watch mocks/data.json",
    "start:proxy": "ng serve --proxy-config proxy.conf.json",
    "start:proxy:mock:server": "concurrently --kill-others \"npm run mock:server\" \"npm run start:proxy\"",
    "concat:mocks": "node \"./mocks/concat-json.js\"",
    "start:server": "npm run concat:mocks && npm run start:proxy:mock:server"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.6",
    "@angular/cdk": "^10.2.7",
    "@angular/common": "~10.0.6",
