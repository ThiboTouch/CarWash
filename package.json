    "@angular/core": "~10.0.6",
    "@angular/forms": "~10.0.6",
    "@angular/google-maps": "^11.0.0",
    "@angular/material": "^10.2.7",
    "@angular/platform-browser": "~10.0.6",
    "@angular/platform-browser-dynamic": "~10.0.6",
    "@angular/router": "~10.0.6",
    "@ngrx/effects": "^10.0.1",
    "@ngrx/schematics": "^10.0.1",
    "@ngrx/store": "^10.0.1",
    "bootstrap": "4.1.1",
    "ngx-bootstrap": "^6.1.0",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
